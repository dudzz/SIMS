<form>

    <div class="row">
        <h3 class="header smaller lighter blue">Upload Allocation Rules</h3>
        <div class="col-xs-6">
            <label class="lbl" for="ProfitCenterGroupList">Profit Center Group:</label>
            <select id="ProfitCenterGroupList" class="form-control">
                <option value="Select">Select</option>
            </select>
        </div>
        <div class="col-xs-6">
            <label class="lbl" for="FiscalYearMonthList">Fiscal Year and Month:</label>
            <select id="FiscalYearMonthList" class="form-control">
                <option value="Select">Select</option>
            </select>
        </div>
        <div class="col-xs-6">
            <label class="lbl" for="UploadTypeList">Upload Type:</label>
            <select id="UploadTypeList" class="form-control">
                <option value="Select">Select</option>
            </select>
        </div>
        <div class="col-xs-12">
            <label class="lbl" for="file">Select Excel File:</label>
            <input type="file" id="file" />
        </div>
        <div class="col-xs-12">
            <button type="submit" class="btn btn-sm btn-success">
                Upload
                <i class="ace-icon fa fa-arrow-up icon-on-right bigger-110"></i>
            </button>
            <button type="button" class="btn btn-sm btn-success">
                Download Template
                <i class="ace-icon fa fa-arrow-down icon-on-right bigger-110"></i>
            </button>
            <button type="reset" class="btn btn-sm btn-success" onclick="Clear()">
                Clear
                <i class="ace-icon fa fa-trash-o icon-on-right bigger-110"></i>
            </button>
        </div>
    </div>

</form>



