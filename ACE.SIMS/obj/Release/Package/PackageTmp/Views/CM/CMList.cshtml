<script type="text/javascript">
    $(function () {
        $('#uploadSuccess').hide();
        //$('#uploadError').hide();

        // ace
        $('#file').ace_file_input({
            no_file: 'No File ...',
            btn_choose: 'Choose',
            btn_change: 'Change',
            droppable: false,
            onchange: null,
            thumbnail: false //| true | large
            //whitelist:'gif|png|jpg|jpeg'
            //blacklist:'exe|php'
            //onchange:''
            //
        });



        AjaxCall('/Allocation/GetProfitCenterGroups', null).done(function (response) {
            if (response.length > 0) {
                var ddlProfitCenterGroups = $("#ProfitCenterGroupList");

                //ddlProfitCenterGroups.empty().append('<option selected="selected" value="0">Please select</option>');
                ddlProfitCenterGroups.empty(); //.append('<option selected="selected" value="0">Please select</option>');
                var i = 0;
                var defaultProfitCenter;
                $.each(response, function () {
                    ddlProfitCenterGroups.append($("<option></option>").val(this['Value']).html(this['Text']));
                    if (i == 0)
                        defaultProfitCenter = this['Value'];

                    i++;
                });

                var profitCenterGroup = defaultProfitCenter;
                FillFYFM(profitCenterGroup);
                FillUploadType(0);
            }
        }).fail(function (error) {
            alert(error.StatusText);
        });


        $('#ProfitCenterGroupList').on("change", function () {
            var profitCenterGroup = $('#ProfitCenterGroupList').val();
            FillFYFM(profitCenterGroup);
            FillUploadType(0);            
        });

        $('#FiscalYearMonthList').on("change", function () {
            var selectedIdx = $('#FiscalYearMonthList').get(0).selectedIndex;
            FillUploadType(selectedIdx);            
        });


    });

    /*******************************************************************
    Generic call to AJAX
    *******************************************************************/
    function AjaxCall(url, data, type) {
        return $.ajax({
            url: url,
            type: type ? type : 'GET',
            data: data,
            contentType: 'application/json'
        });
    }

    /*******************************************************************
    Fill the Fiscal Year / Month Dropdown List
    *******************************************************************/
    function FillFYFM(profitCenterGroup) {
        var obj = { profitCenterGroup: profitCenterGroup };
        AjaxCall('/Allocation/GetFiscalYearMonths', JSON.stringify(obj), 'POST').done(function (response) {
            if (response.length > 0) {
                var ddlFiscalYearMonths = $("#FiscalYearMonthList");
                ddlFiscalYearMonths.empty();
                $.each(response, function () {
                    ddlFiscalYearMonths.append($("<option></option>").val(this['Value']).html(this['Text']));
                });
            }
        }).fail(function (error) {
            alert(error.StatusText);
        });
    }

    /*******************************************************************
    Fill the Upload Type Dropdown List
    *******************************************************************/
    function FillUploadType(selectedIdx) {
        var obj = { selectedIndex: selectedIdx };
        AjaxCall('/Allocation/GetUploadTypes', JSON.stringify(obj), 'POST').done(function (response) {
            if (response.length > 0) {
                var ddlUploadTypes = $("#UploadTypeList");
                ddlUploadTypes.empty();
                $.each(response, function () {
                    ddlUploadTypes.append($("<option></option>").val(this['Value']).html(this['Text']));
                });
            }
        }).fail(function (error) {
            alert(error.StatusText);
        });
    }

    /*******************************************************************
    Clear the Upload File
    *******************************************************************/
    function Clear() {
        $('#file').ace_file_input('reset_input');
    }

</script>




